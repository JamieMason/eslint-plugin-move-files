{
  "name": "eslint-plugin-move-files",
  "description": "Move and rename files while keeping imports up to date",
  "version": "0.7.2",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "bugs": "https://github.com/JamieMason/eslint-plugin-move-files/issues",
  "dependencies": {
    "fs-extra": "8.1.0",
    "glob": "7.1.4"
  },
  "devDependencies": {
    "@types/eslint": "4.16.6",
    "@types/fs-extra": "8.0.0",
    "@types/glob": "7.1.1",
    "@types/jest": "24.0.16",
    "@types/mock-fs": "3.6.30",
    "@types/node": "12.6.9",
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "mock-fs": "4.10.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/JamieMason/eslint-plugin-move-files",
  "keywords": [
    "es6",
    "eslint",
    "eslint-plugin"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "repository": "JamieMason/eslint-plugin-move-files",
  "resolutions": {
    "lodash": ">=4.17.15"
  },
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write '**/*.md' './src/**/*.ts' './test/**/*.ts'",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prebuild": "rimraf ./dist",
    "prelint": "npm run format",
    "prepack": "npm run build",
    "test": "jest --runInBand"
  }
}
